#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/marcos-vini/Documentos/www/my-portfolio-nuxt-official/node_modules/.pnpm/vite-node@2.1.4_@types+node@20.11.17_terser@5.36.0/node_modules/vite-node/node_modules:/home/marcos-vini/Documentos/www/my-portfolio-nuxt-official/node_modules/.pnpm/vite-node@2.1.4_@types+node@20.11.17_terser@5.36.0/node_modules:/home/marcos-vini/Documentos/www/my-portfolio-nuxt-official/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/marcos-vini/Documentos/www/my-portfolio-nuxt-official/node_modules/.pnpm/vite-node@2.1.4_@types+node@20.11.17_terser@5.36.0/node_modules/vite-node/node_modules:/home/marcos-vini/Documentos/www/my-portfolio-nuxt-official/node_modules/.pnpm/vite-node@2.1.4_@types+node@20.11.17_terser@5.36.0/node_modules:/home/marcos-vini/Documentos/www/my-portfolio-nuxt-official/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
